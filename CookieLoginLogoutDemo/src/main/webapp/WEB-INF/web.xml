<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>CookieLoginLogoutDemo</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
     <servlet-name>LoginServlet</servlet-name>
   <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>org.com.cookie.fsg.LoginServlet</servlet-class>
    </servlet>
    
     <welcome-file-list>
    <welcome-file>link.html</welcome-file>
   </welcome-file-list>
     <servlet-mapping>
     <servlet-name>LogOutServlet</servlet-name>
   <url-pattern>./servlet2</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>LogOutServlett</servlet-name>
    <servlet-class>org.com.cookie.fsg.LogOutServlet</servlet-class>
    </servlet>
    
     <welcome-file-list>
    <welcome-file>login.html</welcome-file>
   </welcome-file-list>
     <servlet-mapping>
     <servlet-name>ProfileServlet</servlet-name>
   <url-pattern>./servlet3</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>ProfileServlet</servlet-name>
    <servlet-class>org.com.cookie.fsg.ProfileServlet</servlet-class>
    </servlet>
</web-app>